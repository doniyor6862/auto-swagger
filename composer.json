{
    "name": "doniyor6862/auto-swagger-laravel",
    "description": "Automatically generate OpenAPI/Swagger documentation for Laravel applications using PHP attributes",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "swagger", "openapi", "api", "documentation", "attributes"],
    "autoload": {
        "psr-4": {
            "Laravel\\AutoSwagger\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Anvarov",
            "email": "doniyor.anvarov@bellissimo.uz"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^10.0",
        "symfony/finder": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0",
        "barryvdh/laravel-ide-helper": "^2.12"
    },
    "suggest": {
        "barryvdh/laravel-ide-helper": "^2.12 - Allows automatic PHPDoc generation for models that can be used for documentation"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Laravel\\AutoSwagger\\Providers\\AutoSwaggerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
